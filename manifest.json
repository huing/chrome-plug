{
  "manifest_version": 3,
  "name": "cookiesMerchant",
  "description": "开发小工具",
  "version": "1.0",
  "action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "cookies",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback",
    "declarativeNetRequestWithHostAccess"
  ],
  "host_permissions": ["*://*.leoao.com/*"],
  "background": {
    "service_worker": "bg.js"
  }
}
